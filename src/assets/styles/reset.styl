body, h1, h2, h3, h4, h5, h6, hr, p, blockquote, dl, dt, dd, ul, ol, li, pre, fieldset, legend, button, input, textarea, th, td
  /* table elements 表格元素 */
  margin 0
  padding 0
  border none
  outline none
html, body
  width 100%
  height 100%
  font-family 'Microsoft YaHei'
  overflow hidden
html::webkit-scrollbar
  width 0px
li
  list-style none
a
  text-decoration none
